  "scripts": {
    "build": "ttsc --project tsconfig.$([ ! -z $APPLITOOLS_WEBDRIVERIO_MAJOR_VERSION ] && echo v$APPLITOOLS_WEBDRIVERIO_MAJOR_VERSION || echo build).json",
